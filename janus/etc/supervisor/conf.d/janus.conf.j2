[program:_janus]
command=/usr/local/bin/janus -F /etc/janus
umask=022
priority=30
autostart=true
autorestart=true
startsecs=5
startretries=5
stopsignal=INT
stopwaitsecs=5
stopasgroup=true
killasgroup=true
stdout_logfile=/dev/fd/1
stdout_logfile_maxbytes=0
stderr_logfile=/dev/fd/2
stderr_logfile_maxbytes=0

[group:_janus]
programs=_janus
priority=30
