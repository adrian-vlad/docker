[eventlistener:_process_listener]
command=python3 /opt/scripts/supervisor/process_listener.py
umask=022
priority=1
autostart=true
autorestart=true
startsecs=5
startretries=5
stopsignal=INT
stopwaitsecs=5
stopasgroup=true
killasgroup=true
stderr_logfile=/dev/fd/2
stderr_logfile_maxbytes=0

buffer_size=100
events=PROCESS_STATE_FATAL,TICK_60
