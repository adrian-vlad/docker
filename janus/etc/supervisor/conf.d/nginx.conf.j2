[program:_nginx]
# everything that impacts the disk besides the recording must have low priority
command=nice -n19 /usr/sbin/nginx -g "daemon off;"
umask=022
priority=40
autostart=true
autorestart=true
startsecs=5
startretries=5
stopsignal=INT
stopwaitsecs=5
stopasgroup=true
killasgroup=true
stdout_logfile=/dev/fd/1
stdout_logfile_maxbytes=0
stderr_logfile=/dev/fd/2
stderr_logfile_maxbytes=0

[group:_nginx]
programs=_nginx
priority=40
